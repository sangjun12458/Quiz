#include <iostream>
#define MAX 1001
using namespace std;

int n;
int arr[MAX];
int DP[MAX];

void Solution()
{
    cin >> n;
    for (int i = 1; i <= n; i++) cin >> arr[i];
    DP[0] = 0;
    DP[1] = arr[1];
    int max = 1;
    for (int i = 2; i <= n; i++)
    {
        for (int j = max; j >= 0; j--)
        {
            if (arr[i] > DP[j])
            {
                DP[j + 1] = arr[i];
                if (j == max) max++;
                break;
            }
        }
    }
    cout << max;
}
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    Solution();
    return 0;
}
